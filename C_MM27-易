#include <iostream>
using namespace std;

int main() {
    int n, m;
    int sum = 0;
    cin >> n >> m;
    if (n < m) {
        for (int i = n; i <= m; ++i) {
            sum += i;
        }
    } else {
        for (int i = m; i <= n; ++i) {
            sum += i;
        }
    }
    cout << sum << endl;

    return 0;
}
