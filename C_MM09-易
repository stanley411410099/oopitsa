#include <iostream>
using namespace std;

int main() {
    int i;
    cin >> i;

    int sum = 1;
    if (i > 31) {
        cout << "Value of more than 31" << endl;
    } else {
        for (int j = 1; j <= i; j++) {
            sum *= 2;
        }
        cout << sum << endl;
    }

    return 0;
}
